{
  "name": "ez-ladder-syntax",
  "displayName": "EZ Ladder Syntax",
  "description": "Syntax highlighting for EZ Ladder Structured Text (ST).",
  "version": "1.0.0",
  "publisher": "AmbitiousApprentice",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ezladder",
        "aliases": [
          "EZ Ladder ST",
          "ezladder"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ezladder",
        "scopeName": "source.ezladder",
        "path": "./syntaxes/ezladder.tmLanguage.json"
      }
    ]
  },
  "scripts": {
  },
  "devDependencies": {
    "vscode": "^1.1.34"
  }
}
